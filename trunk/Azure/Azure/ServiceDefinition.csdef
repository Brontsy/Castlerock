<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="Castlerock.Portal" vmsize="ExtraSmall">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
      <Site name="Auslink" physicalDirectory="D:\Github\Castlerock\trunk\Auslink\Auslink.Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" hostHeader="www.auslinkproperty.com.au" />
        </Bindings>
      </Site>
      <Site name="Castlerock" physicalDirectory="D:\Github\Castlerock\trunk\Castlerock\Castlerock.Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" hostHeader="www.castlerockproperty.com.au" />
        </Bindings>
      </Site>
      <Site name="Auslink Portal" physicalDirectory="D:\Github\Castlerock\trunk\Portal\Portal.Web\Portal.Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" hostHeader="portal.auslinkproperty.com.au" />
        </Bindings>
      </Site>
      <Site name="Castlerock Portal" physicalDirectory="D:\Github\Castlerock\trunk\Portal\Portal.Web\Portal.Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" hostHeader="portal.castlerockproperty.com.au" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="BlobConnectionString" />
    </ConfigurationSettings>
  </WebRole>
</ServiceDefinition>