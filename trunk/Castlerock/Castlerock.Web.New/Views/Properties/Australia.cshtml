@model Castlerock.Web.New.Models.PropertyPageViewModel



<div id="AustralianMap">
    
        @foreach (System.Collections.Generic.KeyValuePair<string, string> keyValue in Model.States)
        {
    <div class="@keyValue.Key.ToLower() state @(Model.State.ToLower() == keyValue.Key.ToLower() ? " selected" : "")">
        
        <a href="@Url.RouteUrl("PropertiesFromState", new { state = keyValue.Key })" class="hidden">
            <img src="http://castlerockproperty.blob.core.windows.net/castlerock/images/australia/@(keyValue.Key.ToLower()).png" />
        </a>

    </div> 
        }  

</div>
    
<script type="text/javascript"> 
 
window.addEvent('domready', function() {
	
	var australianMap = new AustralianMap($$('.state'));
		
});
 
</script>          