
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="aboutUs">
    <div class="headerImage">
    
    </div>
</div>

@{IList<Portal.FileManager.Interfaces.IStorageItem> items = (IList<Portal.FileManager.Interfaces.IStorageItem>)ViewData["StorageItems"];}

<div id="content">
    <h2>Downloads</h2>

    <ul>
    @foreach (var item in items)
    {
        <li>
            <a href="@item.Uri">@item.Name</a>
        </li>
    }
    </ul>
</div>