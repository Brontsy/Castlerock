
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Auslink Property: Administrator</title>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,400italic,700,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:300,200,100' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900' rel='stylesheet' type='text/css'>


    <link rel="stylesheet" href="/Areas/Admin/Content/less/site.less" type="text/css" charset="utf-8" />


    @Scripts.Render("~/admin/bundles/modernizr")
    <!-- scripts -->
    @Scripts.Render("~/admin/bundles/jquery")
    @Scripts.Render("~/admin/bundles/jqueryval")
    @Scripts.Render("~/admin/bundles/scripts")
    @RenderSection("scripts", required: false)
    <script src="//code.jquery.com/ui/1.11.0/jquery-ui.js"></script>

</head>
<body>

    <div class="header">
        <h1>Auslinkproperty</h1>

        <ul class="logged-in-member">
            <li class="dropdown profile_menu">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i>@{Html.RenderAction("LoggedInMember", "Membership");}<b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>@Html.RouteLink("Logout", AdminRoutes.Logout)</li>
                </ul>
            </li>
        </ul>

    </div>

    <div class="float-container">

        <div class="sidebar left">
            <div class="sidebar-content left">
                <ul class="navigation">
                    <li>
                        <a href="@Url.RouteUrl(AdminRoutes.Home, new { })"><i class="fa fa-home"></i> Home</a>
                    </li>
                    <li>
                        <a href="@Url.RouteUrl(AdminRoutes.Membership.Index)"><i class="fa fa-users"></i> Membership</a>
                    </li>
                    <li>
                        <a href="@Url.RouteUrl(AdminRoutes.QuarterlyUpdates.Index)"><i class="fa fa-refresh"></i> Quarterly Updates</a>
                    </li>
                    <li>
                        <h4>Website</h4>
                        <ul>
                            <li>
                                <a href="@Url.RouteUrl(AdminRoutes.Cms.ViewPage, new { name = "home", pageId = "996C83AF-4D1D-41A1-AB1A-4BD56D59AAAB" })">- Homepage</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>

        <div class="page-content">
            @RenderBody()
        </div>

    </div>


</body>
</html>
